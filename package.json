{
  "name": "vkma-ui",
  "version": "1.4.1",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "git@github.com:wolframdeus/vkma-ui.git",
  "author": "Vladislav Kibenko <wolfram.deus@gmail.com>",
  "license": "MIT",
  "files": [
    "dist/**/*"
  ],
  "keywords": [
    "vk-mini-apps",
    "vk",
    "vkma",
    "vkui"
  ],
  "scripts": {
    "start": "react-scripts start",
    "build": "gulp",
    "test": "tsc -p tsconfig-build.json --noEmit",
    "prepare": "install-peers",
    "lint": "eslint src/**/*.{ts,tsx}",
    "validate": "npm-run-all lint test",
    "commit": "git-cz",
    "prerelease": "npm-run-all validate build",
    "release": "standard-version -a",
    "release:tags": "git push --follow-tags origin master && yarn publish --non-interactive",
    "release:github": "dotenv -e .env -- conventional-github-releaser -p eslint",
    "postrelease": "npm-run-all release:*"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/classnames": "^2.2.10",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-transition-group": "^4.2.4",
    "@typescript-eslint/eslint-plugin": "^2.33.0",
    "@typescript-eslint/parser": "^2.33.0",
    "commitizen": "^4.1.2",
    "conventional-github-releaser": "^3.1.3",
    "cz-conventional-changelog": "^3.2.0",
    "dotenv": "^8.2.0",
    "dotenv-cli": "^3.1.0",
    "eslint": "^6.8.0",
    "eslint-plugin-react": "^7.20.0",
    "gulp": "^4.0.2",
    "gulp-clean": "^0.4.0",
    "gulp-minify": "^3.1.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "husky": "^4.2.5",
    "install-peers-cli": "^2.2.0",
    "lint-staged": "^10.2.2",
    "npm-run-all": "^4.1.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scripts": "^3.4.1",
    "standard-version": "^8.0.0",
    "typescript": "^3.9.2",
    "vkma-router": "^1.2.2"
  },
  "peerDependencies": {
    "@material-ui/styles": "^4.9.14",
    "@vkontakte/vk-bridge": "^2.2.1",
    "react": "^16.13.1"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{ts,tsx}": [
      "eslint --fix"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "react-transition-group": "^4.4.1"
  }
}
